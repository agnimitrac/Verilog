TD_tb_simpleFifo.read_fifo ;
    %load/vec4 v0x555556c908e0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555556c90aa0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555556c90aa0_0, 0, 1;
